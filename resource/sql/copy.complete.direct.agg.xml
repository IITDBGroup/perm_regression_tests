<entry key="q6.query">
SELECT PROVENANCE ON CONTRIBUTION (copy complete nottransitive) sum(r.i), s.i FROM r, s WHERE r.i &lt; s.i GROUP BY s.i;
</entry>
<entry key="q6.result">
 sum | i | prov_public_r_i | prov_public_s_i 
-----+---+-----------------+-----------------
   6 | 4 |                 |               4
   6 | 4 |                 |               4
   6 | 4 |                 |               4
   1 | 2 |                 |               2
</entry>

<entry key="q7.query">
SELECT PROVENANCE ON CONTRIBUTION (copy complete nottransitive) sum(sub.one), sub.grp 
FROM (SELECT count(*) AS one, s.i AS grp FROM r, s WHERE r.i = s.i GROUP BY s.i) AS sub
GROUP BY sub.grp;
</entry>
<entry key="q7.result">
 sum | grp | prov_public_r_i | prov_public_s_i 
-----+-----+-----------------+-----------------
   1 |   1 |                 |               1
   1 |   2 |                 |               2
</entry>

<entry key="q8.query">
SELECT PROVENANCE ON CONTRIBUTION (copy complete nottransitive) suma, grpa FROM (SELECT sum(r.i) AS suma, r.i AS grpa FROM r GROUP BY r.i) AS sub;
</entry>
